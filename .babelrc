{
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-dynamic-import"
],
  "presets": [
  ["env",
    {
      "useBuiltIns": true,
      "forceAllTransforms": true,
      "ignoreBrowserslistConfig": false,
      "modules": "commonjs",
      "debug": false,
      "uglify": true,
      "browsers": ["last 2 versions", "not ie <= 11"],
      "loose": true,
      "targets": {
        "node": "current",
      }
    }],
  "jest"
  ]
}
